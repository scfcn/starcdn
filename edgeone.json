{
  "functions": [
    {
      "path": "/npm/*",
      "function": "jsdelivr-proxy"
    },
    {
      "path": "/gh/*",
      "function": "jsdelivr-proxy"
    }
  ],
  
  "headers": [
    {
      "source": "/npm/*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000"
        }
      ]
    },
    {
      "source": "/gh/*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000"
        }
      ]
    }
  ]
}
